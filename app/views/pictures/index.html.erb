<h1>新規投稿</h1>
<div class="col-sm-12"><%= image_tag @picture_last.image , :class => 'img img-rounded'%></div>
<%= form_for(@picture, url: upload_pictures_path, method: :post) do |f| %>
  
  <!-- 入力フォーム -->
  <%= f.label :"写真を選択" %>
  <%= f.file_field :image %>
  <br>
  <%= f.submit '投稿', :class => "btn btn-primary" %>
  <%= link_to '戻る',:back, :class => 'btn btn-default' %>
<% end %>